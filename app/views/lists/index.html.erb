<% if @lists.present? %>
    <% @lists.each do |list| %>
        <%= list.created_at.strftime("%A,  %b %d,  %Y") %> <%=link_to "#{list.title}", user_list_path(current_user, list)%><br><br>
    <% end %>
<% end %>

<%= link_to "Back to #{current_user.name.capitalize}'s Page", user_path(current_user) %>
