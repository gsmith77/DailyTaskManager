<%= form_for user do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name%><br><br>
    <%= f.label "Password"%>
    <%= f.password_field :password %><br><br>
    <%= f.label :email %>
    <%= f.email_field :email %><br>
    <%= f.label "Bio" %><br>
    <%= f.text_area :bio %><br><br>
    <%= f.submit "Create User"%>
<% end %>