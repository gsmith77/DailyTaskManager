<%= form_for @task, url: list_create_task_path(List.find(params[:list_id])) do |f| %>
    <%= f.label "Content" %>
    <%= f.text_field :content %><br><br>
    Which List would you like to add this task to? <%= select_tag :title, options_from_collection_for_select(List.all, :title, :title) %>
    <%= f.submit "Create Task" %>
<% end %>