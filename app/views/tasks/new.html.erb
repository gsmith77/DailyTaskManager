<h3><%=@list.title%></h3>
<%= flash[:alert] %><br><br>
<%= form_for @task, url: list_create_task_path(List.find(params[:list_id])) do |f| %>
    <%= f.label "Content" %>
    <%= f.text_field :content %><br><br>
    <%= f.submit "Create Task" %>
<% end %>

<hidden_field_tag id=<%=@list.id %>>
<hidden_field_tag id=<%=current_user.id %>>

